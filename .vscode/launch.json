    "version": "0.2.0",
    "configurations": [
        {
            "name": "TiTiler EOPF Local",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "titiler.eopf.main:app",
                "--reload",
                "--port", "8000",
                "--log-level", "info",
                "--access-log"
            ],
            "console": "integratedTerminal",
            "jinja": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/.venv/bin",
                "LOG_LEVEL": "INFO",
                "TITILER_EOPF_STORE_URL": "file:///home/emathot/Workspace/eopf-explorer/data-model/tests-output/",
            },
        },
        {
            "name": "TiTiler EOPF S3",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "titiler.eopf.main:app",
                "--reload",
                "--port", "8000",
                "--log-level", "info",
                "--access-log"
            ],
            "console": "integratedTerminal",
            "jinja": true,
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/.venv/bin",
                "LOG_LEVEL": "INFO",
                "AWS_PROFILE": "eopf-explorer",
                "AWS_DEFAULT_REGION": "de",
                "AWS_ENDPOINT_URL": "https://s3.de.io.cloud.ovh.net/",
                "AWS_EC2_METADATA_DISABLED": "true",
                "TITILER_EOPF_STORE_URL": "s3://esa-zarr-sentinel-explorer-fra/tests-output/"
            },
        }
    ]
}

