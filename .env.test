# Test environment for cache validation using launch.json AWS settings
AWS_PROFILE=eopf-explorer-emmanuel
AWS_DEFAULT_REGION=de
AWS_ENDPOINT_URL=https://s3.de.io.cloud.ovh.net/
AWS_EC2_METADATA_DISABLED=true

# EOPF Cache Settings  
TITILER_EOPF_CACHE_ENABLE=True
TITILER_EOPF_CACHE_HOST=localhost
TITILER_EOPF_CACHE_BACKEND=s3-redis
TITILER_EOPF_CACHE_REDIS_URL=redis://localhost:6379
TITILER_EOPF_CACHE_DEFAULT_TTL=3600
TITILER_EOPF_CACHE_NAMESPACE=titiler-eopf

# S3 Cache Storage Settings
TITILER_EOPF_CACHE_S3_BUCKET=esa-sentinel-zarr-explorer-cache
TITILER_EOPF_CACHE_S3_REGION=de
TITILER_EOPF_CACHE_S3_ENDPOINT_URL=https://s3.de.io.cloud.ovh.net/

# Store settings from launch.json
TITILER_EOPF_STORE_URL=s3://esa-zarr-sentinel-explorer-fra/tests-output/

# OpenEO Settings
TITILER_OPENEO_STAC_API_URL=http://api.explorer.eopf.copernicus.eu/stac/
TITILER_OPENEO_STORE_URL=services/eopf-explorer.json